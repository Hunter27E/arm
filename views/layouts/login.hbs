		<script src="https://code.jquery.com/jquery.js"></script>
		<script src="#"></script>
	</head>
	<body>
		<div class="container my-container">
			<div class="form-row">
				<input
					id="email"
					name="email"
					type="email"
					placeholder="email address"
					class="form-control"
					required
				/>
			</div>
			<div class="form-row">
				<input
					id="password"
					name="password"
					type="password"
					placeholder="*******"
					type="password"
					class="form-control"
					required
				/>
			</div>
			<div class="form-row">
				<a href="index">
					<button type="submit" class="btn btn-primary">Login</button>
				</a>
			</div>
		</div>
	</body>
</html>
